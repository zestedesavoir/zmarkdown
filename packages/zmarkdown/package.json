  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run server && eslint .",
    "test": "cross-env DEST=/tmp jest",
    "posttest": "npm run stop",
    "watch": "jest --watch",
    "server": "pm2 start -f server/index.js -i 3 --max-memory-restart 150M",
    "stop": "pm2 kill",
    "watch:client": "cross-env NODE_ENV=development webpack --watch",
    "release": "webpack"
  },
